{% extends 'WTSemestralkaBundle::layout.html.twig' %}
{% block content %}
<h1>Model creation</h1>

<form action="{{ path('model_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('model') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock content %}